{
  "name": "keysers-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "pkill -f 'next-server' || true && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "kill-dev": "pkill -f 'next-server' && echo 'Killed all Next.js dev servers' || echo 'No dev servers running'",
    "check-port": "sudo ss -tlnp | grep :3000 || echo 'Port 3000 is free'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "db:setup": "prisma migrate dev && prisma generate && npm run prisma:seed"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.16.0",
    "axios": "^1.13.4",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.13",
    "pg": "^8.18.0",
    "prisma": "5.22.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.1.0",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
